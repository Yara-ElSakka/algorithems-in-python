listOftodos = ["go to the pool", "shopping with mom", "visit grandma" ]
listOftodos.reverse()
print(listOftodos)

def reverseNum(x):

    string = str(x)
    if string[0] == "-":
        return int("-" + string[:0:-1])
    else:
        return int(string[::-1])

print(reverseNum(1122334455))

